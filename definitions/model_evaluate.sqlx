config {
  type: 'table',
  name: 'model_evaluate',
  assertions: {
    rowConditions: [
      'accuracy > .80'
    ]
  }
}

SELECT
  *
FROM
  ML.EVALUATE (
    MODEL ${ref('bq_model')},
    (
      SELECT
        *
      FROM
        ${ref('census_input')}
      WHERE
        dataframe = 'evaluation'
    )
  )
